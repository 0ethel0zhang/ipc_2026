{
  "manifest_version": 3,
  "name": "Image Downloader Agent",
  "version": "1.1",
  "description": "Downloads specific images to a custom folder.",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Download Images"
  }
}